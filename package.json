{
  "name": "mutalyzer-sadi",
  "version": "0.1.0",
  "description": "a SADIfied Mutalyzer",
  "main": "dist/server/server.js",
  "scripts": {
    "build-server": "webpack; node dist/tools/buildSwaggerJSON.js",
    "build-docs": "cp -r src/docs dist",
    "build": "npm run build-server; npm run build-docs",
    "prestart": "npm run build",
    "start": "forever start -a -l forever.log -o out.log -e err.log ./dist/server/server.js",
    "stop": "forever stop dist/server/server.js",
    "prerestart": "npm run build",
    "restart": "forever restart -a ./dist/server/server.js",
    "test": "mocha dist/test/test.js"
  },
  "keywords": [
    "mutalyzer",
    "SADI"
  ],
  "authors": [
    "Michiel Helvensteijn <mhelvens@gmail.com>"
  ],
  "dependencies": {
    "co": "~4",
    "commander": "~2",
    "es6-promisify": "~3",
    "express": "~4",
    "forever": "~0.15",
    "handlebars": "^4.0.4",
    "lodash": "~3",
    "serve-favicon": "^2.3.0",
    "soap": "~0.11",
    "soap-as-promised": "~1",
    "swagger-express-middleware": "~1"
  },
  "devDependencies": {
    "babel": "~5",
    "babel-core": "~5",
    "babel-loader": "~5",
    "chai": "~3",
    "file-loader": "^0.8.4",
    "json-loader": "~0.5",
    "mocha": "~2",
    "raw-loader": "^0.5.1",
    "supertest": "~1",
    "supertest-as-promised": "~2",
    "swagger": "~0.7",
    "swagger-ui": "~2",
    "webpack": "~1"
  }
}
